{
    "name": "injection-vscode",
    "displayName": "Injection.VsCode",
    "description": "Yoko Injection script extension for Visual Studio Code",
    "publisher": "uoinfusion",
    "version": "0.0.16",
    "qna": "marketplace",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:injection"
    ],
    "repository": {
        "url": "https://github.com/uoinfusion/InjectionScript/tree/master/clients/vscode"
    },
    "license": "Apache License 2.0",
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "contributes": {
        "languages": [
            {
                "id": "injection",
                "aliases": [
                    "injection",
                    "injection"
                ],
                "extensions": [
                    "sc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "injection",
                "scopeName": "source.injection",
                "path": "./syntaxes/injection.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "vscode-languageclient": "^4.1.4"
    },
    "devDependencies": {
        "@types/node": "^6.0.45",
        "tslint": "^5.12.0",
        "typescript": "^3.2.2",
        "vscode": "^1.1.26"
    }
}
